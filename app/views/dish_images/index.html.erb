<p id="notice"><%= notice %></p>

<h1>Dish Images</h1>

<table>
  <thead>
    <tr>
      <th>Dish</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dish_images.each do |dish_image| %>
      <tr>
        <td><%= dish_image.dish_id %></td>
        <td><%= dish_image.url %></td>
        <td><%= link_to t('show'), dish_image %></td>
        <td><%= link_to 'Edit', edit_dish_image_path(dish_image) %></td>
        <td><%= link_to 'Destroy', dish_image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dish Image', new_dish_image_path %>
