<p id="notice"><%= notice %></p>

<h1>chefs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Lastname</th>
      <th>Username</th>
      <th>Email</th>
      <th>Birthday</th>
      <th>Phone</th>
      <th>Picture</th>
      <th>Type</th>
      <th>Category</th>
      <th>Bio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chefs.each do |chef| %>
      <tr>
        <td><%= chef.name %></td>
        <td><%= chef.lastname %></td>
        <td><%= chef.username %></td>
        <td><%= chef.email %></td>
        <td><%= chef.birthday %></td>
        <td><%= chef.phone %></td>
        <td><%= chef.picture %></td>
        <td><%= chef.type %></td>
        <td><%= chef.category %></td>
        <td><%= chef.bio %></td>

        <td> <a href= "<%= chef_path(chef) %>" class="btn  btn-lg mybutton_cyano wow fadeIn" data-wow-delay="0.8s"><span class="network-name">Ver perfil</span></a> </td>
        <td> <br> <hr> </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
