<p id="notice"><%= notice %></p>
  <div class="container-field">
    <div class="col-md-12">
    <div class="col-md-7 dish-page">
      <div class="col-md-12 ">
        <%=render "dishes/show_dish", dish: @dish, chef: @dish.chef, divide: " - "  %>
      </div>
    </div>
    <div class="col-md-4 comment">
      <H3>Comentarios</H3>
      <%@dish_evaluations.each do |evaluation| %>
        <%=render "dish_evaluations/show", dish_evaluation: evaluation%>
      <%end%>
    </div>
  </div>

    <a href= "<%= dishes_path %>" class="btn btn-lg mybutton_cyano wow fadeIn col-md-6" data-wow-delay="0.8s"><span class="network-name">Ver todos</span></a>
    <a href= "<%= chef_dishes_path(@dish.chef) %>" class="btn  btn-lg mybutton_cyano wow fadeIn col-md-6" data-wow-delay="0.8s"><span class="network-name">Platos de este chef</span></a>

  </div>
